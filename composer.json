{
  "name": "parspack/acme-client",
  "description": "LetsEncrypt client library for ACME v2 written in PHP.",
  "homepage": "https://github.com/ali-nikookherad-abrha/acme-client.git",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "psr/log": "*",
    "spatie/dns": "*",
    "spatie/laravel-data": "*"
  },
  "require-dev": {
    "larapack/dd": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Rogierw\\RwAcme\\": "src"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
